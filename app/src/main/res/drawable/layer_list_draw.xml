<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@android:color/transparent"></solid>
        </shape>
    </item>
    <item>
        android:bottom="12dp"
        android:left="12dp"
        android:right="12dp"
        android:top="12dp"
        <shape>
            <solid android:color="#E0EEE0"></solid>
            <stroke
                android:width="4dp"
                android:color="#E0EEE0"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
</layer-list>
